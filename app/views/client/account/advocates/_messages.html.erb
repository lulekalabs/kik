<div class="light">
  <div class="main">
    <div class="content">
      <div class="col_1"> 
        <%= render :partial => "shared/advocates/overview", :object => @advocate %>
        <%= render :partial => "shared/advocates/grade", :object => @advocate %>  

        <% if current_user.person.is_a?(Client) %>
          <%= render :partial => "client/account/advocates/tabs" %>
        <% end %>
        
        <% if current_user.person.is_a?(Advocate) %>
          <%= render :partial => "advocate/account/previews/tabs" %>
        <% end %>

        <%= render :partial => "client/account/advocates/content_messages" %> 


        <%= render :partial => "client/account/advocates/regulate" %>
      </div>

      <div class="col_2">
        <%= render :partial => "shared/advocates/sidebar_location" %>
      </div>

    </div>

  </div>
  <div class="bottom"></div>
</div>




