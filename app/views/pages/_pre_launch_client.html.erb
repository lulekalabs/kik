      <%   link_to(url_for("http://twitter.com/kannichklagen" ) , :target=>'blank') do %>
        <img title="twitter" alt="twitter" src="/images/logos/twitter.gif" style="float:left"/>
      <% end %>
      <%   link_to(url_for("http://www.facebook.com/kannichklagen" ) , :target=>'blank') do %>
        <img title="facebook" alt="facebook" src="/images/logos/facebook.gif" style="float:left"/>
    <% end %></div>
    <div class="two_bottom"></div>
 
  <div class="clearer_10"></div>  
 </div>